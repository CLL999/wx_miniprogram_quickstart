{
  "name": "uni-preset-vue",
  "version": "0.0.0",
  "scripts": {
    "gcg": "graphql-codegen",
    "format": "prettier --write ./src",
    "lint": "eslint src --ext .ts,.vue",
    "dev:app": "uni -p app",
    "dev:h5": "uni",
    "dev:h5:ssr": "uni --ssr",
    "dev:mp": "uni -p mp-weixin",
    "prod:app": "uni -p app --mode production",
    "prod:h5": "uni --mode production",
    "prod:h5:ssr": "uni --ssr --mode production",
    "prod:mp": "uni -p mp-weixin --mode production",
    "build:app": "uni build -p app --mode production",
    "build:h5": "uni build --mode production",
    "build:h5:ssr": "uni build --ssr --mode production",
    "build:mp": "uni build -p mp-weixin --mode production",
    "open:dev": "weapp open -p dist/dev/mp-weixin",
    "open:build": "weapp open -p dist/build/mp-weixin"
  },
  "dependencies": {
    "@apollo/client": "^3.6.8",
    "@dcloudio/uni-app": "3.0.0-alpha-3041720220614001",
    "@dcloudio/uni-app-plus": "3.0.0-alpha-3041720220614001",
    "@dcloudio/uni-components": "3.0.0-alpha-3041720220614001",
    "@dcloudio/uni-h5": "3.0.0-alpha-3041720220614001",
    "@dcloudio/uni-mp-weixin": "3.0.0-alpha-3041720220614001",
    "@vue/apollo-composable": "4.0.0-alpha.17",
    "dayjs": "^1.11.4",
    "graphql": "^16.5.0",
    "graphql-ws": "^5.9.0",
    "immer": "^9.0.15",
    "pinia": "^2.0.14",
    "promise-polyfill": "^8.2.3",
    "svgtofont": "^3.17.8",
    "vue": "^3.2.37",
    "vue-i18n": "^9.1.9"
  },
  "devDependencies": {
    "@dcloudio/types": "^3.0.14",
    "@dcloudio/uni-automator": "3.0.0-alpha-3041720220614001",
    "@dcloudio/uni-cli-shared": "3.0.0-alpha-3041720220614001",
    "@dcloudio/vite-plugin-uni": "3.0.0-alpha-3041720220614001",
    "@graphql-codegen/add": "^3.1.1",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typed-document-node": "^2.2.13",
    "@graphql-codegen/typescript": "^2.5.1",
    "@graphql-codegen/typescript-operations": "^2.4.2",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/node": "^18.0.6",
    "@types/promise-polyfill": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "@vue/eslint-config-standard-with-typescript": "^8.0.0",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.2.0",
    "postcss": "^8.4.14",
    "postcss-rem-to-responsive-pixel": "^5.1.3",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.3",
    "uni-base-components-types": "^1.1.1",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.4",
    "vite": "^2.9.12",
    "weapp-tailwindcss-webpack-plugin": "^1.9.2"
  }
}
